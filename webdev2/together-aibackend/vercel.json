{
  "version": 2,
  "builds": [
    {
      "src": "api/L22.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
